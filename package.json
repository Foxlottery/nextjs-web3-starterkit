{
  "name": "foxlottery-interface",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "yarn lingui && next build",
    "export": "yarn build && next export",
    "start": "next start",
    "lint": "next lint",
    "prettier": "prettier --write 'src/**/*.{ts,tsx}'",
    "prepare": "husky install",
    "pre-commit": "lint-staged",
    "lingui": "lingui extract --clean --overwrite && lingui compile"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{md,css}": "prettier --write"
  },
  "dependencies": {
    "next": "12.1.5",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@babel/preset-react": "^7.16.7",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@fontsource/dm-sans": "^4.5.7",
    "@foxlottery/core-sdk": "^1.0.0-canary.94",
    "@headlessui/react": "^1.6.0",
    "@heroicons/react": "^1.0.6",
    "@lingui/cli": "^3.12.1",
    "@lingui/core": "^3.12.1",
    "@lingui/loader": "^3.12.1",
    "@lingui/macro": "^3.12.1",
    "@lingui/react": "^3.12.1",
    "@lingui/remote-loader": "^3.11.0",
    "@sentry/nextjs": "^6.19.7",
    "@sentry/wizard": "^1.2.17",
    "@types/node": "17.0.24",
    "@types/react": "18.0.5",
    "@types/react-dom": "18.0.1",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@welldone-software/why-did-you-render": "^6.2.3",
    "autoprefixer": "^10.4.5",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-macros": "^3.1.0",
    "eslint": "8.13.0",
    "eslint-config-next": "12.1.5",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-unused-imports": "^1.1.5",
    "husky": "^7.0.4",
    "lint-staged": "^12.4.0",
    "make-plural": "^7.1.0",
    "next-seo": "^5.4.0",
    "postcss": "^8.4.0",
    "postcss-flexbugs-fixes": "^5.0.2",
    "postcss-preset-env": "^7.4.0",
    "prettier": "^2.6.2",
    "react-responsive": "^9.0.0-beta.6",
    "react-virtualized": "^9.22.3",
    "tailwindcss": "^3.0.24",
    "tailwindcss-border-gradient-radius": "^3.0.1",
    "typescript": "4.6.3"
  },
  "entrypoints": [
    "src/pages"
  ]
}
